<div class="container" style="margin-top: 150px;">
    <div class="row">
        <div class="col-6">
            <p>Ajouté le : {{ product.createdAt|date('d-m-Y') }}</p>
            <p>Modifié le : {{ product.ModifiedAt|date('d-m-Y') }}</p>
            {{ form_start(form) }}
            {{ form_row(form.category) }}
            {{ form_rest(form) }}
            <button class="btn btn-secondary" value="{{ button|default('Ajouter') }}"></button>
            {{ form_end(form) }}

        </div>
        <div class="col-4">
            <img src="{{ product.img }}" width="558" >
        </div>
    </div>
</div>